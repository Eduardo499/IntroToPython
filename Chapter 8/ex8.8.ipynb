    "def display_emoji_table():\n",
    "    print(f\"{'Code':<10}{'Emoji':<10}\")\n",
    "    print(\"-\" * 20)\n",
    "\n",
    "    for i in range(0x1F600, 0x1F600 + 10):\n",
    "        emoji = chr(i)\n",
    "        print(f\"{hex(i):<10}{emoji:<10}\")\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    display_emoji_table()"
