    "from dataclasses import dataclass\n",
    "\n",
    "@dataclass\n",
    "class Card:\n",
    "    face: str \n",
    "    suit: str \n",
    "\n",
    "card1 = Card(face=2, suit=3)\n",
    "card2 = Card(face=10, suit=11)\n",
    "\n",
    "print(card1)\n",
    "print(card2)"
