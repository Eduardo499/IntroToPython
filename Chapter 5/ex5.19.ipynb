    "    (\"Michael\", \"Jones\"),\n",
    "    (\"Sarah\", \"Brown\"),\n",
    "    (\"Chris\", \"Jones\"),\n",
    "]\n",
    "\n",
    "jones = list(filter(lambda x: x[1] == 'Jones', names))\n",
