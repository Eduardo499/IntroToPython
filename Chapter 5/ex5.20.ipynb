   ],
   "source": [
    "def display_table(tables):\n",
    "    for i in tables:\n",
    "        for j in i:\n",
    "            print(f'{j:>4}', end='')\n",
    "        print()\n",
    "\n",
    "tables = [[i * j for j in range(1, 11)] for i in range(1, 11)]\n",
    "\n",
    "display_table(tables)"
   ]
  }
