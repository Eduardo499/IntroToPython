    "from math import pi\n",
    "\n",
    "def rad(degrees):\n",
    "    return degrees * pi / 180\n",
    "\n",
    "for i in range(180):\n",
    "    print(f'{i + 1}° = {rad(i + 1):.2f}')\n"
